<?xml version="1.0"?>
    <routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/demoapi/name" method="GET">
        <service class="Embitel\Mymodule\Api\DemoInterface" method="getProfile"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/demoapi/hello" method="POST">
        <service class="Embitel\Mymodule\Api\DemoInterface" method="getInfo"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    </routes>
    
